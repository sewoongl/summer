<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="aop">

	<insert id="insert" parameterType="HashMap">
	 	insert into aop 
			(methodNM, menu, `type`) 
		values 
			(#{methodNM}, #{menu}, #{type})
	</insert>
	
	<select id="select">
		select * form aop
	</select>



</mapper>